{
        "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.venv/": true,
        "**/__pycache__/": true,
        "**/.gitignore": true,
        "**/.robotName": true,
        "**/.robotNameList": true,
    },
    "git.postCommitCommand": "sync",
    "task.allowAutomaticTasks": "on",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "editor.rulers":[
        80
    ]
}
